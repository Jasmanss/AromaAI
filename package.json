{
  "name": "aromaai",
  "version": "1.0.0",
  "description": "Fragrance recommendation system using ML and computer vision",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "flask": "python ml_server.py",
    "dev:all": "concurrently \"npm run dev\" \"npm run flask\"",
    "build": "echo 'Building for production...' && mkdir -p dist && cp -r public/* dist/ && mkdir -p dist/data && cp data/fragrances.json dist/data/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "fragrance",
    "recommendation",
    "machine-learning",
    "computer-vision"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.8.4",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "fs": "^0.0.1-security",
    "mongoose": "^8.12.2",
    "multer": "^1.4.5-lts.2",
    "openai": "^4.89.0",
    "path": "^0.12.7",
    "python-shell": "^5.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.1.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jasmanss/AromaAI.git"
  },
  "bugs": {
    "url": "https://github.com/Jasmanss/AromaAI/issues"
  },
  "homepage": "https://jasmanss.github.io/AromaAI"
}
